body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

.content
  text-align center

nav
  text-align center
  a
    display inline-block
    line-height 1
    padding 10px 20px
    text-decoration none
    border 1px solid #00b7ff
    border-radius 3px
    margin : 10px 10px
    transition all ease 0.05s
    cursor pointer
    &:hover
      background #00b7ff
      color #fff
